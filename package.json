{
  "name": "gadgetexplorer",
  "displayName": "gadgetExplorer",
  "description": "",
  "publisher": "Mentats",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gadgetExplorer",
          "title": "Gadget Explorer",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "gadgetExplorer": [
        {
          "id": "gadget-files-tree",
          "name": "Gadget Files"
        }
      ]
    },
    "languages": [
      {
        "id": "gadgetsFile",
        "aliases": [
          "ropGadgets"
        ],
        "extensions": [
          "gadgets.txt"
        ]
      }
    ],
    "commands": [
      {
        "command": "gadgetExplorer.gadgetfile.createSnapshot",
        "title": "Create Snapshot",
        "icon": "$(triangle-right)"
      },
      {
        "command": "gadgetExplorer.gadgetfile.loadSnapshot",
        "title": "Load previous Snapshot",
        "icon": "$(triangle-left)"
      },
      {
        "command": "gadgetExplorer.test",
        "title": "Test Command",
        "icon": "media/icon.svg"
      },
      {
        "command": "gadgetExplorer.highlight.red",
        "title": "Highlight Red"
      },
      {
        "command": "gadgetExplorer.highlight.green",
        "title": "Highlight Green"
      },
      {
        "command": "gadgetExplorer.highlight.blue",
        "title": "Highlight Blue"
      },
      {
        "command": "gadgetExplorer.highlight.yellow",
        "title": "Highlight Yellow"
      },
      {
        "command": "gadgetExplorer.highlight.orange",
        "title": "Highlight Orange"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "gadgetExplorer.test",
          "when": "view == gadget-files-tree",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "gadgetExplorer.gadgetfile.loadSnapshot",
          "when": "view == gadget-files-tree && viewItem == gadgetFile",
          "group": "inline"
        },
        {
          "command": "gadgetExplorer.gadgetfile.createSnapshot",
          "when": "view == gadget-files-tree && viewItem == gadgetFile",
          "group": "inline"
        }
      ],
      "commandPalette": [
        {
          "command": "gadgetExplorer.test",
          "when": "gadgetExplorer.gadgetFileFlag"
        },
        {
          "command": "gadgetExplorer.highlight.red",
          "title": "Highlight Row",
          "when": "gadgetExplorer.gadgetFileFlag"
        },
        {
          "command": "gadgetExplorer.highlight.green",
          "title": "Highlight Green",
          "when": "gadgetExplorer.gadgetFileFlag"
        },
        {
          "command": "gadgetExplorer.highlight.blue",
          "title": "Highlight Blue",
          "when": "gadgetExplorer.gadgetFileFlag"
        },
        {
          "command": "gadgetExplorer.highlight.yellow",
          "title": "Highlight Yellow",
          "when": "gadgetExplorer.gadgetFileFlag"
        },
        {
          "command": "gadgetExplorer.highlight.orange",
          "title": "Highlight Orange",
          "when": "gadgetExplorer.gadgetFileFlag"
        }
      ]
    },
    "keybindings": [
      {
        "command": "gadgetExplorer.highlight.red",
        "key": "q",
        "when": "gadgetExplorer.gadgetFileFlag"
      },
      {
        "command": "gadgetExplorer.highlight.green",
        "key": "w",
        "when": "gadgetExplorer.gadgetFileFlag"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}